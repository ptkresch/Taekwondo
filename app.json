{
  "expo": {
    "name": "ptc",
    "slug": "ptc",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/ptc_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ptkresch.ptc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ptc_logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": "Powell's Taekwondo Center - App Build",
    "githubUrl": "https://github.com/ptkresch/Taekwondo"
  }
}
